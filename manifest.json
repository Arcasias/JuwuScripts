{
  "name": "Scripts",
  "description": "Bunch of useful and quirky Scripts",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "extension/background.js"
  },
  "permissions": ["background", "activeTab", "tabs", "scripting", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "action": {
    "default_popup": "./extension/build/index.html"
  }
}
